        <option name="-s">../../../trunk/examples/settings/automizer/concurrent/svcomp-Reach-32bit-Automizer_Default-noMmResRef-FA-VariableLbe-McrStrategy.epf</option>
	</rundefinition>

    <rundefinition name="Automizer FA-NoLbe-MCR">
        <option name="-s">../../../trunk/examples/settings/automizer/concurrent/svcomp-Reach-32bit-Automizer_Default-noMmResRef-FA-NoLbe-McrStrategy.epf</option>
	</rundefinition>

    
	<tasks name="SVCOMP ConcurrencySafety-Main">
		<includesfile>../../../trunk/examples/svcomp/ConcurrencySafety-Main.set</includesfile>
		<propertyfile>../../../trunk/examples/svcomp/properties/unreach-call.prp</propertyfile>
	</tasks>
    
	<tasks name="Ultimate Repo">
		<includesfile>../../../trunk/examples/bench-pthreads.set</includesfile>
		<propertyfile>../../../trunk/examples/svcomp/properties/unreach-call.prp</propertyfile>
	</tasks>

</benchmark>
