    <rundefinition name="Automizer SVCOMP 2020">
        <option name="-s">../../../trunk/examples/settings/default/automizer/svcomp-Reach-32bit-Automizer_Default.epf</option>
    </rundefinition>

    <rundefinition name="Automizer FA-SemanticLbe">
        <option name="-s">../../../trunk/examples/settings/automizer/concurrent/svcomp-Reach-32bit-Automizer_Default-noMmResRef-FA-SemanticLbe.epf</option>
	</rundefinition>

    <rundefinition name="Automizer FA-VariableLbe">
        <option name="-s">../../../trunk/examples/settings/automizer/concurrent/svcomp-Reach-32bit-Automizer_Default-noMmResRef-FA-VariableLbe.epf</option>
	</rundefinition>

    <rundefinition name="Automizer FA-NoLbe">
        <option name="-s">../../../trunk/examples/settings/automizer/concurrent/svcomp-Reach-32bit-Automizer_Default-noMmResRef-FA-NoLbe.epf</option>
	</rundefinition>
    
    <rundefinition name="Automizer FA-SemanticLbe-MCR">
        <option name="-s">../../../trunk/examples/settings/automizer/concurrent/svcomp-Reach-32bit-Automizer_Default-noMmResRef-FA-SemanticLbe-McrStrategy.epf</option>
