<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<benchmark tool="ultimateautomizer" memlimit="8GB" timelimit="300" hardtimelimit="320" cpuCores="2">
    
	<option name="--force-no-wrapper" />
