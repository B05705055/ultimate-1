<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<fragment>
   <extension
         point="local.stalin.ep.source">
      <impl
            class="local.stalin.SMTInterface.test.SMTInterfaceTest">
      </impl>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            default="false"
            id="local.stalin.SMTInterface.viewer.ProofTreeEditor"
            name="Z3 Proof Tree Editor"
            class="local.stalin.SMTInterface.viewer.ProofTreeEditor">
      </editor>
   </extension>
<extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Action Set"
            visible="true"
            id="local.stalin.SMTInterfaceTest.actionSet">
         <menu
               label="SMTInterfaceVisualization"
               id="smtinterfacevisualization">
            <separator
                  name="smtvisgroup">
            </separator>
         </menu>

         
         <action
               class="local.stalin.SMTInterface.viewer.SaveDisplayAction"
               id="local.stalin.SMTInterface.export.SaveDisplayAction"
               label="Save Display"
               menubarPath="smtinterfacevisualization/smtvisgroup"/>
         
      </actionSet>
   </extension>
</fragment>
