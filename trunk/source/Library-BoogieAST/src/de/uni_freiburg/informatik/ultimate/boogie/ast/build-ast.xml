<project name="proj1" default="generate_ast" basedir=".">
    <property name="astbuilderDir" location="../../../../../../../../ASTBuilder/bin"/>
    <property name="cupDir" location="../../../../../../../../JavaCup/bin"/>
	
    <description>
        Build File for Compiler Project
    </description>

    <target name="generate_ast">
    	<echo>Classpath will be set to ${astbuilderDir}:${cupDir}</echo>
        <java classname="de.uni_freiburg.informatik.ultimate.astbuilder.Main" classpath="${astbuilderDir}:${cupDir}" fork="true">
        	<arg value="-ultimatenew" />
            <arg value="Boogie.ast" />
        </java>
    </target>

</project>