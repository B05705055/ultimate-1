<project name="SpaceEx_Parser" default="spaceex_parser" basedir=".">
	<record name="${workspacedir}/antbuild-spaceexparser.log" />
	<!-- this expects the binary xjc to be known / to be in the PATH on your system,
		 but if you really use some JDK 6 (what you are required to do anyway) that
		 should automatically be the case on Unix machines -->
	<!-- Beware Windows user: You might have to add JDK1.6.X/bin to your PATH environment variable -->
	<target name="spaceex_parser">
		<echo>Building the SpaceEx File Parser</echo>
		<exec executable="xjc">
			<arg line="-d ${workspacedir}/SpaceExParser/src -p de.uni_freiburg.informatik.ultimate.plugins.spaceex.parser.generated sspaceex.xsd" />
		</exec>
	</target>

</project>