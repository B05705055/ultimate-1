<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="de.uni_freiburg.informatik.ultimate.ep.analysis" name="Analysis Extension Point" schema="schema/de.uni_freiburg.informatik.ultimate.ep.analysis.exsd"/>
   <extension-point id="de.uni_freiburg.informatik.ultimate.ep.generator" name="Generator Extension Point" schema="schema/de.uni_freiburg.informatik.ultimate.ep.generator.exsd"/>
   <extension-point id="de.uni_freiburg.informatik.ultimate.ep.output" name="Output Extension Point" schema="schema/de.uni_freiburg.informatik.ultimate.ep.output.exsd"/>
   <extension-point id="de.uni_freiburg.informatik.ultimate.ep.LoggingWindow" name="LoggingWindow Extension Point" schema="schema/de.uni_freiburg.informatik.ultimate.ep.LoggingWindow.exsd"/>
   <extension-point id="de.uni_freiburg.informatik.ultimate.ep.controller" name="Controller Extension Point" schema="schema/de.uni_freiburg.informatik.ultimate.ep.controller.exsd"/>
   <extension-point id="de.uni_freiburg.informatik.ultimate.ep.source" name="Source Extension Point" schema="schema/de.uni_freiburg.informatik.ultimate.ep.source.exsd"/>

   <extension id="CoreOSGi" point="org.eclipse.core.runtime.applications">
      <application cardinality="*" thread="any" visible="true">
         <run class="de.uni_freiburg.informatik.ultimate.core.coreplugin.Application"/>   
      </application>
   </extension>
   
   <extension id="CoreRCP" point="org.eclipse.core.runtime.applications">
      <application cardinality="*" thread="main" visible="true">
         <run class="de.uni_freiburg.informatik.ultimate.core.coreplugin.Application"/>   
      </application>
   </extension>
   
   <extension point="org.eclipse.core.runtime.products" id="Debug">
      <product application="UltimateCore.CoreRCP" name="Ultimate-Debug">
         <property name="appName" value="Ultimate-Debug" />
          <property name="aboutImage" value="icons/about.gif" />
         <property name="aboutText"
               value="Ultimate Ultimate is a tool for model checking and static program analysis. It provides multiple toolchains which check properties or transform input files." />
        </product>
   </extension>
   
    <extension point="org.eclipse.core.runtime.products" id="CLI">
      <product application="UltimateCore.CoreRCP" name="Ultimate-CommandLine">
         <property name="appName" value="Ultimate-CommandLine" />
         <property name="aboutImage" value="icons/about.gif" />
         <property name="aboutText"
               value="Ultimate Ultimate is a tool for model checking and static program analysis. It provides multiple toolchains which check properties or transform input files." />
      </product>
   </extension>
  
  
   <extension point="org.eclipse.core.runtime.preferences">
      <initializer class="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.CorePreferenceInitializer"/>
   </extension>
  
   <extension point="org.eclipse.ui.preferencePages">
      <page
            class="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.UltimatePreferencePage"
            id="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.UltimatePreferencePage"
            name="Ultimate Preferences"/>
      <page
            category="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.UltimatePreferencePage"
            class="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.OutputPreferencePage"
            id="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.OutputPreferencePage"
            name="Output Plugins"/>
      <page
            category="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.UltimatePreferencePage"
            class="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.ToolsPreferencePage"
            id="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.ToolsPreferencePage"
            name="Tool Plugins"/>            
      <page
            category="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.UltimatePreferencePage"
            class="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.CorePreferencePage"
            id="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.CorePreferencePage"
            name="Core Preferences"/>
      <page
            category="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.CorePreferencePage"
            class="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.LoggingPreferencePage"
            id="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.LoggingPreferencePage"
            name="Log Levels" />
      
      <page
            category="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.LoggingPreferencePage"
            class="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.LoggingDetailsPreferencePage"
            id="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.LoggingDetailsPreferencePage"
            name="Plug-in-specific Log Levels" />
      
      <page
            category="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.LoggingPreferencePage"
            class="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.LoggingToolsPreferencePage"
            id="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.LoggingToolsPreferencePage"
            name="Log Levels for external tools" />
      
      <page
            category="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.CorePreferencePage"
            class="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.ModelManagerPreferencePage"
            id="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.ModelManagerPreferencePage"
            name="Model Manager" />
      
      <page
            category="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.CorePreferencePage"
            class="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.LogFilePreferencePage"
            id="de.uni_freiburg.informatik.ultimate.core.coreplugin.preferences.LogFilePreferencePage"
            name="Log File" />      
   </extension>
</plugin>

