<?xml version="1.0" encoding="iso-8859-1"?>
<sspaceex xmlns="http://www-verimag.imag.fr/xml-namespaces/sspaceex" version="0.2" math="SpaceEx">
  <component id="heater">
    <param name="x" type="real" local="false" d1="1" d2="1" dynamics="any" />
    <param name="y" type="real" local="false" d1="1" d2="1" dynamics="any" />
    <param name="time" type="real" local="false" d1="1" d2="1" dynamics="any" />
    <param name="trans12" type="label" local="false" />
    <param name="trans23" type="label" local="false" />
    <param name="trans34" type="label" local="false" />
    <param name="trans45" type="label" local="false" />
    <param name="trans56" type="label" local="false" />
    <param name="trans67" type="label" local="false" />
    <location id="1" name="phase1" x="187.5" y="291.5" width="367.0" height="103.0">
      <invariant>1.0 &lt;= x &lt;= 2.0 &amp; 1.0 &lt;= y &lt;= 2.0 &amp; 0.0 &lt;= time &lt;= 1.0</invariant>
      <flow>x' == 1.0 &amp; y' == 1.0 &amp; time' == 1.0</flow>
    </location>
    <location id="2" name="phase2" x="527.0" y="292.0" width="190.0" height="68.0">
      <invariant>1.0 &lt;= time &lt;= 3.0</invariant>
      <flow>x' == 1.0 &amp; y' == 0.0 &amp; time' == 1.0</flow>
    </location>
    <location id="3" name="phase3" x="753.0" y="291.0" width="206.0" height="60.0">
      <invariant>3.0 &lt;= time &lt;= 4.0</invariant>
      <flow>x' == 1.0 &amp; y' == 0.0-1.0 &amp; time' == 1.0</flow>
    </location>
    <location id="4" name="phase4" x="775.0" y="470.0" width="126.0" height="76.0">
      <invariant>4.0&lt;=time&lt;=5.0</invariant>
      <flow>x'==0.0-1.0 &amp; y'==0.0-1.0</flow>
    </location>
    <location id="5" name="phase5" x="551.0" y="511.0" width="184.0" height="78.0">
      <invariant>5.0&lt;=time&lt;=7.0</invariant>
      <flow>x'==0.0-1.0 &amp; y'==0.0 &amp; time'==1.0</flow>
    </location>
    <location id="6" name="phase6" x="268.0" y="518.0" width="200.0" height="74.0">
      <invariant>7.0&lt;=time&lt;=8.0</invariant>
      <flow>x'==0.0-1.0 &amp; y'==1.0 &amp; time'==1.0</flow>
    </location>
    <transition source="1" target="2">
      <label>trans12</label>
      <guard>time == 1.0</guard>
      <labelposition x="-52.0" y="-63.0" width="112.0" height="64.0" />
      <middlepoint x="379.75" y="201.75" />
    </transition>
    <transition source="2" target="3">
      <label>trans23</label>
      <guard>time == 3.0</guard>
      <labelposition x="-67.0" y="-63.0" width="124.0" height="52.0" />
      <middlepoint x="709.5" y="212.5" />
    </transition>
    <transition source="3" target="4">
      <label>trans34</label>
      <guard>time==4.0</guard>
      <labelposition x="5.0" y="-19.0" width="90.0" height="50.0" />
    </transition>
    <transition source="4" target="5">
      <label>trans45</label>
      <guard>time==5.0</guard>
      <labelposition x="3.0" y="36.0" width="74.0" height="52.0" />
    </transition>
    <transition source="5" target="6">
      <label>trans56</label>
      <guard>time==7.0</guard>
      <labelposition x="-37.0" y="26.0" width="84.0" height="52.0" />
    </transition>
    <transition source="6" target="1">
      <label>trans67</label>
      <guard>time==8.0</guard>
      <assignment>time==0.0</assignment>
      <labelposition x="6.0" y="-36.0" width="142.0" height="66.0" />
    </transition>
  </component>
</sspaceex>

